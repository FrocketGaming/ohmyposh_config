{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "overflow": "hide",
      "segments": [
        {
          "background": "#BD93F9",
          "foreground": "#F8F8F2",
          "leading_diamond": "",
          "style": "diamond",
          "template": " {{ .UserName }} ",
          "trailing_diamond": "",
          "type": "session"
        },
        {
          "background": "#FF79C6",
          "foreground": "#F8F8F2",
          "powerline_symbol": "",
          "properties": {
            "folder_separator_icon": " ",
            "home_icon": "~",
            "style": "folder"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#F1FA8C",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFB86C{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FF5555{{ end }}",
            "{{ if gt .Ahead 0 }}#50FA7B{{ end }}",
            "{{ if gt .Behind 0 }}#BD93F9{{ end }}"
          ],
          "foreground": "#282A36",
          "leading_diamond": "",
          "powerline_symbol": "",
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} {{ .StashCount }}{{ end }} ",
          "trailing_diamond": "",
          "type": "git"
        },
        {
          "background": "#50FA7B",
          "foreground": "#282A36",
          "powerline_symbol": "",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#8BE9FD",
          "foreground": "#282A36",
          "powerline_symbol": "",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "go"
        },
        {
          "background": "#BD93F9",
          "foreground": "#282A36",
          "powerline_symbol": "",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "julia"
        },
        {
          "background": "#FFB86C",
          "foreground": "#282A36",
          "powerline_symbol": "",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#F1FA8C",
          "foreground": "#282A36",
          "powerline_symbol": "",
          "style": "powerline",
          "template": " ",
          "type": "root"
        },
        {
          "background": "#50FA7B",
          "background_templates": [
            "{{ if gt .Code 0 }}#FF5555{{ end }}"
          ],
          "foreground": "#282A36",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": "<parentBackground></> ",
          "trailing_diamond": "",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#BD93F9",
          "style": "plain",
          "template": "â†’ ",
          "type": "text"
        }
      ],
      "type": "prompt",
      "newline": true
    },
    {
      "segments": [
        {
          "background": "#8BE9FD",
          "foreground": "#282A36",
          "style": "plain",
          "template": "<#8BE9FD,transparent></> {{ .Name }} <transparent,#8BE9FD></>",
          "type": "shell"
        },
        {
          "background": "#BD93F9",
          "foreground": "#282A36",
          "invert_powerline": true,
          "leading_diamond": "",
          "style": "diamond",
          "template": " {{ .CurrentDate | date \"3:04PM\" }} ",
          "trailing_diamond": "",
          "type": "time"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 2
}
