{
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": false,
      "overflow": "hide",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "background": "#BD93F9",
          "foreground": "#F8F8F2",
          "template": " {{ .UserName }} "
        },
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "background": "#FF79C6",
          "foreground": "#F8F8F2",
          "properties": {
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "style": "folder"
          },
          "template": " \uea83  {{ .Path }} "
        },
        {
          "type": "git",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "background": "#F1FA8C",
          "foreground": "#282A36",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFB86C{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FF5555{{ end }}",
            "{{ if gt .Ahead 0 }}#50FA7B{{ end }}",
            "{{ if gt .Behind 0 }}#BD93F9{{ end }}"
          ],
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} "
        },
        {
          "type": "node",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "background": "#50FA7B",
          "foreground": "#282A36",
          "properties": {
            "fetch_version": true
          },
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} "
        },
        {
          "type": "go",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "background": "#8BE9FD",
          "foreground": "#282A36",
          "properties": {
            "fetch_version": true
          },
          "template": " \ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "julia",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "background": "#BD93F9",
          "foreground": "#F8F8F2",
          "properties": {
            "fetch_version": true
          },
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "python",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "background": "#FFB86C",
          "foreground": "#282A36",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "root",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "background": "#F1FA8C",
          "foreground": "#282A36",
          "template": " \uf0ad "
        },
        {
          "type": "status",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "background": "#50FA7B",
          "foreground": "#282A36",
          "background_templates": [
            "{{ if gt .Code 0 }}#FF5555{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "template": " \ue23a "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#BD93F9",
          "template": "â†’ "
        }
      ]
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "type": "shell",
          "style": "plain",
          "background": "#8BE9FD",
          "foreground": "#282A36",
          "template": "<#8BE9FD,transparent>\ue0b6</> \uf489 {{ .Name }} <transparent,#8BE9FD>\ue0b2</>"
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "background": "#BD93F9",
          "foreground": "#282A36",
          "invert_powerline": true,
          "template": " {{ .CurrentDate | date \"3:04PM\" }} "
        }
      ]
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 2
}
