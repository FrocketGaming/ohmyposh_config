{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "overflow": "hide",
      "segments": [
        {
          "background": "#BD93F9",
          "foreground": "#F8F8F2",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": " {{ .UserName }} ",
          "trailing_diamond": "\ue0b0",
          "type": "session"
        },
        {
          "background": "#FF79C6",
          "foreground": "#F8F8F2",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "style": "folder"
          },
          "style": "powerline",
          "template": " \uea83  {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#F1FA8C",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFB86C{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FF5555{{ end }}",
            "{{ if gt .Ahead 0 }}#50FA7B{{ end }}",
            "{{ if gt .Behind 0 }}#BD93F9{{ end }}"
          ],
          "foreground": "#282A36",
          "leading_diamond": "\ue0b6",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "trailing_diamond": "\ue0b4",
          "type": "git"
        },
        {
          "background": "#50FA7B",
          "foreground": "#282A36",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#8BE9FD",
          "foreground": "#282A36",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "go"
        },
        {
          "background": "#BD93F9",
          "foreground": "#F8F8F2",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "julia"
        },
        {
          "background": "#FFB86C",
          "foreground": "#282A36",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "powerline",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#F1FA8C",
          "foreground": "#282A36",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0ad ",
          "type": "root"
        },
        {
          "background": "#50FA7B",
          "background_templates": [
            "{{ if gt .Code 0 }}#FF5555{{ end }}"
          ],
          "foreground": "#282A36",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": "<parentBackground></> \ue23a ",
          "trailing_diamond": "\ue0b4",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "segments": [
        {
          "background": "#8BE9FD",
          "foreground": "#282A36",
          "style": "plain",
          "template": "<#8BE9FD,transparent>\ue0b6</> \uf489 {{ .Name }} <transparent,#8BE9FD>\ue0b2</>",
          "type": "shell"
        },
        {
          "background": "#FF5555",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#8BE9FD{{end}}",
            "{{if eq \"Discharging\" .State.String}}#FF5555{{end}}",
            "{{if eq \"Full\" .State.String}}#50FA7B{{end}}"
          ],
          "foreground": "#F8F8F2",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "charged_icon": "\ue22f ",
            "charging_icon": "\ue234 ",
            "discharging_icon": "\ue231 "
          },
          "style": "powerline",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 ",
          "type": "battery"
        },
        {
          "background": "#BD93F9",
          "foreground": "#282A36",
          "invert_powerline": true,
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": " {{ .CurrentDate | date \"3:04PM\" }} ",
          "trailing_diamond": "\ue0b4",
          "type": "time"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 2
}
